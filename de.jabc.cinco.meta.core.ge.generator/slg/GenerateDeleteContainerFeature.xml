      <string>package $context.localPkgName$;

import java.util.ArrayList;
import java.util.List;

import graphmodel.Edge;
import graphmodel.Node;
import graphmodel.Container;

import org.eclipse.emf.ecore.EObject;
import org.eclipse.graphiti.features.IDeleteFeature;
import org.eclipse.graphiti.features.IFeatureProvider;
import org.eclipse.graphiti.features.context.IDeleteContext;
import org.eclipse.graphiti.features.context.impl.DeleteContext;
import org.eclipse.graphiti.mm.pictograms.PictogramElement;
import org.eclipse.graphiti.services.Graphiti;
import org.eclipse.graphiti.ui.features.DefaultDeleteFeature;

import $context.importPath$.*;

public class Delete$context.container.name$Feature extends DefaultDeleteFeature {

	public Delete$context.container.name$Feature(IFeatureProvider fp) {
		super(fp);
	}

	public boolean canDelete(IDeleteContext context, boolean apiCall) {
		if (apiCall) {
			if (getBusinessObjectForPictogramElement(context.getPictogramElement()) instanceof $context.container.name$)
				return true;
			return super.canDelete(context);
		}
		return false;
	}
	
	$context.canDeleteContent$

	@Override
	public void delete(IDeleteContext context) {
		EObject bo = (EObject) getBusinessObjectForPictogramElement(context.getPictogramElement());
		if (bo instanceof Container) {
			Container nc = (Container) bo;
			List&lt;Node&gt; delNodes = new ArrayList&lt;Node&gt;();
			delNodes.addAll(nc.getModelElements(Node.class));
			for (Node containedNode : delNodes) {
				PictogramElement containedNodePE = Graphiti.getLinkService().getPictogramElements(getDiagram(), containedNode).get(0);
				DeleteContext delContext = new DeleteContext(containedNodePE);
				IDeleteFeature df = getFeatureProvider().getDeleteFeature(delContext);
				if (df != null &amp;&amp; df.canDelete(delContext)) {
					df.delete(delContext);
				}
			}
			List&lt;Edge&gt; edges = new ArrayList&lt;&gt;();
			edges.addAll(nc.getIncoming());
			edges.addAll(nc.getOutgoing());
			for (Edge e : edges) {
				PictogramElement pe = Graphiti.getLinkService().getPictogramElements(getDiagram(), e).get(0);
				DeleteContext dc = new DeleteContext(pe);
				IDeleteFeature df = getFeatureProvider().getDeleteFeature(dc);
				if (df != null &amp;&amp; df.canDelete(dc))
					df.delete(dc);
			}
		}

		super.delete(context);
	}
	
	
	@Override
	protected boolean getUserDecision(IDeleteContext context) {
		return true;
	}
}
