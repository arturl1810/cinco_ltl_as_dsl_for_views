    <parameter name="template">
      <string>switch (context.getDirection()) {
	case (IResizeShapeContext.DIRECTION_WEST):
	case (IResizeShapeContext.DIRECTION_NORTH):
	case (IResizeShapeContext.DIRECTION_NORTH_WEST):
		break;
	case (IResizeShapeContext.DIRECTION_SOUTH):
		deltaHeight = 0;
	case (IResizeShapeContext.DIRECTION_NORTH_EAST):
		deltaWidth = 0;
		break;
	case (IResizeShapeContext.DIRECTION_SOUTH_WEST):
		deltaHeight = 0;
		break;
	default:
		deltaHeight = 0;
		deltaWidth = 0;
		break;
	}

for (ModelElement me : (($context.importPath$.$context.importPath$.$context.modelElement.name$) bo).getModelElements()) {
	if (me instanceof Edge) {
		List&lt;PictogramElement&gt; pictogramElements = Graphiti.getLinkService().getPictogramElements(getDiagram(), me);
		if (pictogramElements.get(0) instanceof FreeFormConnection) {
			FreeFormConnection conn = (FreeFormConnection) pictogramElements.get(0);
			for (Point p : conn.getBendpoints()) {
				p.setX(p.getX() + deltaWidth);
				p.setY(p.getY() + deltaHeight);
			}
		}
	}
