    <parameter name="template">
      <string>package $context.localPkgName$;

import $context.packageName$.graphiti.*;
import $context.packageName$.graphiti.content.adapter.*;
import $context.importPath$.*;

import org.eclipse.graphiti.features.*;
import org.eclipse.graphiti.features.context.*;
import org.eclipse.graphiti.features.impl.*;
import org.eclipse.graphiti.mm.pictograms.*;

public class Create$context.containerName$Feature extends AbstractCreateFeature {

public Create$context.containerName$Feature(IFeatureProvider fp) {
		super(fp, &quot;$context.containerName$&quot;, &quot;Create $context.containerName$&quot;);
	}

	public boolean canCreate(ICreateContext context) {
		return context.getTargetContainer() instanceof Diagram;
	}

	public Object[] create(ICreateContext context) {
		$context.containerName$ $context.meIdentifier$ = $context.factoryName$Factory.eINSTANCE.create$context.containerName$();
		 
		//TODO: Add for loop over attributes and initialize them
		$context.graphModelName$ $context.lcGraphModelName$ = $context.graphModelName$GraphitiUtils.addToResource(getDiagram(), this.getFeatureProvider());
		$context.lcGraphModelName$.getModelElements().add($context.meIdentifier$);
		addGraphicalRepresentation(context, $context.meIdentifier$);
		
		return new Object[] {$context.meIdentifier$}; 
	}
}</string>
