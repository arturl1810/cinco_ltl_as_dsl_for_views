      <string>private void updateStyle($context.importPath$.$context.fuModelElementName$ bo, Shape s) {
	Diagram d = (getDiagram() == null) ? $context.graphModelName$GraphitiUtils.getInstance().getDTP().getDiagram() : getDiagram();
	String gaName = Graphiti.getPeService().getPropertyValue(s.getGraphicsAlgorithm(), $context.graphModelName$LayoutUtils.KEY_GA_NAME);
	if (gaName != null &amp;&amp; !gaName.isEmpty() &amp;&amp; d != null) {
		$context.graphModelName$LayoutUtils.updateStyleFromAppearance(s.getGraphicsAlgorithm(), new $context.appearanceProvider$().getAppearance(bo, gaName), d);
	}
	Object object = Graphiti.getLinkService().getBusinessObjectForLinkedPictogramElement(s);
	if (object instanceof $context.importPath$.$context.fuModelElementName$) {
		if (s instanceof ContainerShape) {
			for (Shape g : ((ContainerShape) s).getChildren()) {
				updateStyle(($context.importPath$.$context.fuModelElementName$) object, g);
			}
		}
	}
}

private void updateStyle($context.importPath$.$context.fuModelElementName$ bo, Connection s) {
	Diagram d = (getDiagram() == null) ? $context.graphModelName$GraphitiUtils.getInstance().getDTP().getDiagram() : getDiagram();
	String gaName = Graphiti.getPeService().getPropertyValue(s.getGraphicsAlgorithm(), $context.graphModelName$LayoutUtils.KEY_GA_NAME);
	if (gaName != null &amp;&amp; !gaName.isEmpty() &amp;&amp; d != null) {
		$context.graphModelName$LayoutUtils.updateStyleFromAppearance(s.getGraphicsAlgorithm(), new $context.appearanceProvider$().getAppearance(bo, gaName), d);
	}
	Object object = Graphiti.getLinkService().getBusinessObjectForLinkedPictogramElement(s);
	if (object instanceof $context.importPath$.$context.fuModelElementName$) {
		
		for (Shape g : s.getConnectionDecorators()) {
			updateStyle(($context.importPath$.$context.fuModelElementName$) object, g);
		}
		
	}

	if (!(getDiagramBehavior() instanceof DiagramBehavior))
		return;
	DiagramBehavior db = (DiagramBehavior) getDiagramBehavior();
	if (db == null)
		return;
	GraphicalEditPart editPart = db.getEditPartForPictogramElement(s);
	if (editPart == null)
		return;
	int selected_info = editPart.getSelected();
	editPart.setSelected(EditPart.SELECTED_NONE);
	editPart.setSelected(selected_info);
}</string>
