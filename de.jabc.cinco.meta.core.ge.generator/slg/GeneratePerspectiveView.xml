      <string>package $context.packageName$;

import org.eclipse.graphiti.ui.internal.editor.ThumbNailView;
import org.eclipse.ui.IFolderLayout;
import org.eclipse.ui.IPageLayout;
import org.eclipse.ui.IPerspectiveFactory;

public class $context.graphModelName$PerspectiveFactory implements IPerspectiveFactory {

	public static final String ID_PERSPECTIVE = &quot;$context.packageName$.$context.lcGraphModelName$perspective&quot;;

	@Override
	public void createInitialLayout(IPageLayout layout) {
		layout.addView(IPageLayout.ID_PROJECT_EXPLORER, IPageLayout.LEFT, 0.25f, IPageLayout.ID_EDITOR_AREA); 
		layout.addView(ThumbNailView.VIEW_ID, IPageLayout.BOTTOM, 0.55f, IPageLayout.ID_PROJECT_EXPLORER);
		
		IFolderLayout folderLayout = layout.createFolder(&quot;$context.packageName$.$context.lcGraphModelName$.property&quot;, IPageLayout.BOTTOM, 0.75f, IPageLayout.ID_EDITOR_AREA);
		
		/** This command adds the common property view **/
		/*folderLayout.addView(IPageLayout.ID_PROP_SHEET);*/
		folderLayout.addView(&quot;de.jabc.cinco.meta.core.ui.propertyview&quot;);
		folderLayout.addView(IPageLayout.ID_PROBLEM_VIEW);
	}

}</string>
