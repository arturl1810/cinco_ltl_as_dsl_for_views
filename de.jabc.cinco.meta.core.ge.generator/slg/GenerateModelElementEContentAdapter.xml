		Object o = notification.getNotifier();
		if (o instanceof $context.importPath$.$context.fuModelElementName$) {
			$context.importPath$.$context.fuModelElementName$ tmp = ($context.importPath$.$context.fuModelElementName$) o;
			if (getDiagram() == null)
