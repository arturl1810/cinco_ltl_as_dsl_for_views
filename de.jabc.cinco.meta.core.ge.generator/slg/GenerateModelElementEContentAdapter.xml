      <string>private void updateStyle($context.importPath$.$context.fuModelElementName$ bo, Shape s) {
	String gaName = Graphiti.getPeService().getPropertyValue(s.getGraphicsAlgorithm(), $context.graphModelName$LayoutUtils.KEY_GA_NAME);
	if (gaName != null &amp;&amp; !gaName.isEmpty()) {
		$context.graphModelName$LayoutUtils.updateStyleFromAppearance(s.getGraphicsAlgorithm(), new $context.appearanceProvider$().getAppearance(bo, gaName), getDiagram());
	}
	Object object = Graphiti.getLinkService().getBusinessObjectForLinkedPictogramElement(s);
	if (object instanceof $context.importPath$.$context.fuModelElementName$) {
		if (s instanceof ContainerShape) {
			for (Shape g : ((ContainerShape) s).getChildren()) {
				updateStyle(($context.importPath$.$context.fuModelElementName$) object, g);
			}
		}
	}
