      <string>package $context.localPkgName$;
import org.eclipse.graphiti.features.IFeatureProvider;
import org.eclipse.graphiti.features.context.IMoveShapeContext;
import org.eclipse.graphiti.features.impl.DefaultMoveShapeFeature;
import org.eclipse.graphiti.mm.pictograms.Diagram;

import graphmodel.Container;

import $context.importPath$.*;

public class Move$context.container.name$Feature extends DefaultMoveShapeFeature{

	public Move$context.container.name$Feature(IFeatureProvider fp) {
		super(fp);
		// TODO Auto-generated constructor stub
	}
	
	@Override
	public boolean canMoveShape(IMoveShapeContext context) {
		Object bo = getBusinessObjectForPictogramElement(context.getShape());
		Object source = getBusinessObjectForPictogramElement(context.getSourceContainer());
		Object target = getBusinessObjectForPictogramElement(context.getTargetContainer());
		if (source != null &amp;&amp; source.equals(target))
			return true;
		$context.movableContainerContent$
	}
	
	@Override
	public void moveShape(IMoveShapeContext context) {
		Object o = getBusinessObjectForPictogramElement(context.getShape());
		Object source = getBusinessObjectForPictogramElement(context.getSourceContainer());
		Object target = getBusinessObjectForPictogramElement(context.getTargetContainer());
		if (source instanceof Container) {
			Container nc = (Container) source;
			nc.getModelElements().remove(($context.container.name$) o);
		}
		if (source instanceof $context.graphModelName$) {
			$context.graphModelName$ tmp = ($context.graphModelName$) source;
			tmp.getModelElements().remove(($context.container.name$) o);
		}
		if (target instanceof $context.graphModelName$) {
			$context.graphModelName$ tmp = ($context.graphModelName$) target;
			tmp.getModelElements().add(($context.container.name$) o);
		}
		if (target instanceof Container) {
			Container nc = (Container) target;
			nc.getModelElements().add(($context.container.name$) o);
		}
		
		super.moveShape(context);
	}

}
</string>
