		for (PictogramLink pl : $context.graphModelName$GraphitiUtils.getInstance().getDTP().getDiagram().getPictogramLinks())
			for (EObject bo : pl.getBusinessObjects())
				$context.graphModelName$EContentAdapter.getInstance().addAdapter(bo);
		
